public class Problem5 {
	public static void main(String[] args) {
		int count;
		boolean b=true;
		for(int x=1;x>0;x++){
			count=0;
			for(int a=1;a<21;a++){
				if(x%a!=0){
					break;
				}
				if(x%a==0){
					count=count+1;
				}
				if(count==20){
					System.out.println(x);
					b=false;
				}
			}
			if(b==false){
				break;
			}
		}
	}
}
